{
    "env": "production",
    "log": {
        "format": "combined",
        "dir": "../logs"
    },
    "cors": {
        "origin": [
            "https://snekden.io"
        ],
        "credentials": true,
        "methods": [
            "GET",
            "POST"
        ],
        "allowedHeaders": [
            "*"
        ]
    },
    "ssl": {
        "keyPath": "/run/secrets/key",
        "certPath": "/run/secrets/pem_cert"
    },
    "sockets": {
        "pingInterval": 50000,
        "pingTimeout": 10000,
        "emissionInterval": 15000
    }
}