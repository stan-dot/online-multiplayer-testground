{
    "env": "development",
    "log": {
        "format": "dev",
        "dir": "../logs"
    },
    "cors": {
        "origin": [
            "http://localhost:3000"
        ],
        "credentials": true,
        "methods": [
            "GET",
            "POST"
        ],
        "allowedHeaders": [
            "*"
        ]
    },
    "ssl": {
        "keyPath": "/run/secrets/key",
        "certPath": "/run/secrets/pem_cert"
    },
    "sockets": {
        "pingInterval": 50000,
        "pingTimeout": 10000,
        "emissionInterval": 15000
    }
}